class Solution {
public:
    vector<int> majorityElement(vector<int>& nums) {
        int count=0;
        vector<int> res;
        int ans;
        int prev_ans;
        for(int i=0;i<nums.size();i++){
            if(count==0){
                count++;
                ans=nums[i];
            }else{
                if(ans==nums[i]){
                    count++;
                }else{
                    count--;
                }
            }
            if(prev_ans!= ans){
                if(count>=nums.size()/3){
                    res.push_back(ans);
                    prev_ans= ans;
                }
            }
        }
        return res;
    }
};